{
  "name": "notanerd",
  "private": true,
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10",
    "postcss": "^8",
    "tailwindcss": "^4",
    "turbo": "latest",
    "typescript": "^5"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
