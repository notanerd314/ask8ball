{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:shelf": {
      "dependsOn": [],
      "cache": false
    },
    "dev:magic-8-ball": {
      "dependsOn": ["dev:shelf"],
      "cache": false
    },
    "lint": {},
    "test": {}
  }
}
